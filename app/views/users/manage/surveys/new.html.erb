<main class="column">
<nav class="breadcrumb is-medium" aria-label="breadcrumbs">
  <ul>
    <li><%= link_to "Dashboard", root_path %></li>
    <li><%= link_to "Survey Listings", users_manage_surveys_path %></li>
    <li class="is-active"><%= link_to "New" , new_users_manage_survey_path, 'aria-current': "page" %></li>
  </ul>
</nav>
    <div class="level">
        <div class="level-left">
        <div class="level-item">
            <div class="title">New Survey</div>
        </div>
        </div>
        <div class="level-right">
        <div class="level-item">
            <button type="button" class="button is-small">
            March 8, 2017
            </button>
        </div>
        </div>
    </div>
    <%= form_for(@survey, url: users_manage_surveys_path) do |f|%>
        <section class="columns">
            <div class="column is-12">
                <%= link_to 'Back', users_manage_surveys_path, class: 'button is-black'%>
                <%= f.submit 'Create', class: 'button is-primary'%>
            </div>
        </section>
        <%= render '/users/manage/surveys/shared/form', f: f%>
    <% end %>
</main>
<script>
    <%= render '/users/manage/surveys/shared/page.js'%>
</script>