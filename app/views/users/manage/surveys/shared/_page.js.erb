    <% if @survey.present? && @survey.show_final_button == true%>
        $('.final_enable').show()
    <% else %>
        $('.final_enable').hide()
    <% end %>
    <% if @survey.final_button_to_start == true%>
        $('#url-input').hide()
    <% else %>
        $('#url-input').show()
    <% end %>
    $("input[type=radio][class=radio_start]").change(function(){ 
        console.log($(this).val())
        if( $(this).is(":checked") == false ){
            $('#url-input').toggle()
        } else if ($(this).is(":checked") == true) {
            $('#url-input').toggle()
        }
    });
    $('#show-final-button').change(function(){
        this.checked ? $('.final_enable').toggle() : $('.final_enable').toggle();
    });